<div class="container">
  
  {{!-- Menu to search for seeds or products for sale or to add items to database --}}
  <div class="row">
    <div id="read-search-menu" class="col">
      <button id="read-products-btn">Search Products for Sale</button>
      <button id="read-seeds-btn">Search Seeds</button>
    </div>
  </div>


  <div class="search-result" id="product-read-result">
    <table id="products-read-table" class="display">
      <thead>
        <tr>
          <th>ID</th>
          <th>Strain</th>
          <th>Price</th>
          <th>Quantity</th>
          <th>Packaging</th>
          <th>Size &#40gram&#41</th>
          <th>THC %</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>Row 1 Data 1</td>
          <td>Row 1 Data 2</td>
          <td>Row 1 Data 3</td>
          <td>Row 1 Data 4</td>
          <td>Row 1 Data 5</td>
          <td>Row 1 Data 6</td>
        </tr>
      </tbody>
    </table>
  </div>
  <div class="search-result" id="seed-read-result">
    <table id="seeds-read-table" class="display">
      <thead>
        <tr>
          <th>ID</th>
          <th>Strain</th>
          <th>Type</th>
          <th>Quantity</th>
          <th>THC %</th>
          <th>CBD %</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>Row 1 Data 1</td>
          <td>Row 1 Data 2</td>
          <td>Row 1 Data 3</td>
          <td>Row 1 Data 4</td>
          <td>Row 1 Data 5</td>
          <td>Row 1 Data 6</td>
        </tr>
      </tbody>
    </table>
  </div>

</div>